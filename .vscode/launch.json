{
  // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
  // 기존 특성에 대한 설명을 보려면 가리킵니다.
  // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
  "version": "0.2.0",
  "configurations": [
    {
      "type": "android",
      "request": "launch",
      "name": "Android Launch",
      "appSrcRoot": "${workspaceRoot}/app/src/main",
      "apkFile": "${workspaceRoot}/app/build/outputs/apk/debug/app-debug.apk",
      "adbPort": 5037
    },
    {
      "name": "recipe_app",
      "cwd": "recipe_app",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "recipe_app (profile mode)",
      "cwd": "recipe_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "recipe_app (release mode)",
      "cwd": "recipe_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
